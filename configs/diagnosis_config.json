{
  "schema_version": "1.0.0",
  "project": "ai-neo",
  "purpose": "AI-guided diagnosis that follows master-style logic (dynamic questioning) using ONLY repo knowledge: positions.md, shifts.md, methodology.md, question_bank.md, examples_transcripts.md.",
  "language": "ru",
  "diagnosis": {
    "mode": "adaptive_interview",
    "max_questions_total": 20,
    "target_duration_minutes": 25,
    "stop_rules": {
      "min_confirmations_per_top_potential": 3,
      "top_potentials_to_lock": 3,
      "stop_when": [
        "top3_potentials_locked_and_no_major_conflicts",
        "confidence_over_threshold_for_rows_and_columns",
        "question_budget_reached"
      ],
      "hard_stop_at_questions": 20
    }
  },
  "matrix": {
    "potentials": [
      "Янтарь",
      "Шунгит",
      "Цитрин",
      "Изумруд",
      "Рубин",
      "Гранат",
      "Сапфир",
      "Гелиодор",
      "Аметист"
    ],
    "rows": {
      "row1": "СИЛЫ",
      "row2": "ЭНЕРГИЯ",
      "row3": "СЛАБОСТИ"
    },
    "columns": {
      "col1": "ВОСПРИЯТИЕ",
      "col2": "МОТИВАЦИЯ",
      "col3": "ИНСТРУМЕНТ"
    }
  },
  "scoring": {
    "base": {
      "selection_split": true,
      "anti_pattern_penalty_coef": 0.8,
      "time_fast_add": 1.0,
      "time_slow_sub": 1.0
    },
    "question_weights": {
      "block1": {
        "q1": 1.5,
        "q2": 1.25,
        "q3": 1.5,
        "q4": 1.4,
        "q5": 1.25,
        "q6": 1.1,
        "q7": 1.1,
        "q8": 1.3,
        "q9": 1.25,
        "q10": 1.0,
        "q11": 1.0,
        "q12": 1.0
      },
      "generic": {
        "childhood_validation": 1.15,
        "behavior_validation": 1.1,
        "identity_claim": 0.9,
        "values_claim": 0.85,
        "social_desirable_risk": 0.6
      },
      "shifts": {
        "shift_probe": 1.2,
        "shift_contradiction": 1.25
      }
    },
    "text_signals": {
      "enabled": true,
      "keywords_boost": 0.35,
      "sentiment_boost": 0.25,
      "certainty_words_boost": 0.15,
      "example_detail_boost": 0.25,
      "penalties": {
        "abstract_talk": 0.8,
        "contradictory_statements": 0.75
      }
    }
  },
  "mapping": {
    "options_to_potentials": {
      "Янтарь": [
        "порядок",
        "структура",
        "система",
        "организация",
        "разложить по местам",
        "документы",
        "регламент",
        "детали",
        "схема"
      ],
      "Шунгит": [
        "движение",
        "тело",
        "спорт",
        "руки",
        "физически",
        "выносливость",
        "качалка",
        "активность"
      ],
      "Цитрин": [
        "деньги",
        "результат",
        "быстро",
        "эффективность",
        "оптимизация",
        "скорость",
        "сделка",
        "доход"
      ],
      "Изумруд": [
        "красота",
        "гармония",
        "уют",
        "эстетика",
        "дизайн",
        "стиль",
        "атмосфера"
      ],
      "Рубин": [
        "драйв",
        "адреналин",
        "новые места",
        "путешествия",
        "эмоции",
        "перезагрузка",
        "трансформация",
        "приключение"
      ],
      "Гранат": [
        "люди",
        "команда",
        "общение",
        "родство",
        "близкие",
        "забота",
        "поддержка",
        "отношения"
      ],
      "Сапфир": [
        "смысл",
        "идея",
        "концепция",
        "философия",
        "почему так устроено",
        "глубина",
        "мировоззрение"
      ],
      "Гелиодор": [
        "знания",
        "изучение",
        "обучение",
        "объяснять",
        "наставник",
        "курс",
        "развитие",
        "учиться"
      ],
      "Аметист": [
        "цель",
        "стратегия",
        "управление",
        "лидерство",
        "план",
        "координация",
        "вектор",
        "проект"
      ]
    },
    "columns_mapping": {
      "ВОСПРИЯТИЕ": [
        "в каких ситуациях уверен",
        "что легко дается",
        "на что обращаешь внимание в первую очередь"
      ],
      "МОТИВАЦИЯ": [
        "что вдохновляет",
        "где теряешь счет времени",
        "что дает удовлетворение"
      ],
      "ИНСТРУМЕНТ": [
        "за что хвалят",
        "какие результаты лучше других",
        "какие навыки развиты"
      ]
    }
  },
  "adaptive_logic": {
    "question_selection_policy": {
      "start": "broad_energy_then_childhood_then_antipattern",
      "priorities": [
        "resolve_top2_tie",
        "validate_childhood_for_top_candidates",
        "run_antipattern_check_for_false_leaders",
        "probe_shifts_if_conflicts_or_overadaptation",
        "lock_columns_by asking mapped questions"
      ],
      "tie_breakers": [
        "choose_question_with_highest_weight",
        "prefer_behavior_over_identity",
        "prefer_childhood_over_current_role",
        "prefer_antipattern_when client says 'надо/должен'"
      ]
    },
    "shift_detection_triggers": [
      "client says: делаю ради семьи/надо/должен/некогда/так принято",
      "strong results but low enjoyment",
      "claims enjoyment but procrastinates consistently",
      "big gap between 'мечта' and 'реальность'",
      "high anxiety around specific potential-related tasks"
    ],
    "shift_probes": {
      "enabled": true,
      "max_shift_questions": 2,
      "when_to_ask": [
        "after first contradiction",
        "before final locking if profile seems 'socially perfect'"
      ]
    }
  },
  "confidence": {
    "thresholds": {
      "lock_potential": 0.72,
      "lock_row": 0.7,
      "lock_column": 0.68,
      "overall_finish": 0.75
    },
    "calculation": {
      "method": "weighted_evidence",
      "signals": [
        "selection_score",
        "text_keyword_score",
        "childhood_alignment",
        "behavior_alignment",
        "antipattern_support",
        "consistency_score",
        "shift_risk_penalty"
      ],
      "shift_risk_penalty_max": 0.18
    }
  },
  "output": {
    "client_report": {
      "enabled": true,
      "tone": "supportive_clear",
      "include": [
        "top3_potentials",
        "rows_and_columns",
        "short_strengths",
        "energy_recovery_tips",
        "next_step_offer"
      ],
      "exclude": [
        "raw_scores",
        "internal_weights",
        "uncertainty_text"
      ]
    },
    "master_report": {
      "enabled": true,
      "include": [
        "raw_scores_table",
        "evidence_by_question",
        "contradictions_found",
        "shift_hypotheses",
        "recommend_followup_questions",
        "confidence_breakdown"
      ]
    }
  },
  "storage": {
    "answers_format": "event_log",
    "event_log_fields": [
      "timestamp",
      "question_id",
      "question_text",
      "answer_type",
      "selected_options",
      "free_text",
      "derived_potentials",
      "weights_applied"
    ]
  }
}